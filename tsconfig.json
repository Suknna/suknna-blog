{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["node", "vitepress/client", "vite/client"],
    "allowArbitraryExtensions": true
  },
  "include": ["env.d.ts", "**/*.ts", "**/*.tsx", "**/*.vue"],
  "exclude": ["node_modules", "dist"]
}
